<h1>Listing productrequests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Fullfilled</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @productrequests.each do |productrequest| %>
      <tr>
        <td><%= productrequest.name %></td>
        <td><%= productrequest.email %></td>
        <td><%= productrequest.phone %></td>
        <td><%= productrequest.fullfilled %></td>
        <td><%= productrequest.location %></td>
        <td><%= link_to 'Show', productrequest %></td>
        <td><%= link_to 'Edit', edit_productrequest_path(productrequest) %></td>
        <td><%= link_to 'Destroy', productrequest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Productrequest', new_productrequest_path %>
