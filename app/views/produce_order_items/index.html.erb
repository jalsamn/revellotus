<h1>Listing produce_order_items</h1>

<table>
  <thead>
    <tr>
      <th>Producename</th>
      <th>Quantity</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @produce_order_items.each do |produce_order_item| %>
      <tr>
        <td><%= produce_order_item.producename %></td>
        <td><%= produce_order_item.quantity %></td>
        <td><%= produce_order_item.produce_order_id %></td>
        <td><%= link_to 'Show', produce_order_item %></td>
        <td><%= link_to 'Edit', edit_produce_order_item_path(produce_order_item) %></td>
        <td><%= link_to 'Destroy', produce_order_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Produce order item', new_produce_order_item_path %>
