<h1>Listing produce_orders</h1>

<table>
  <thead>
    <tr>
      <th>Vendor</th>
      <th>Orderdate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @produce_orders.each do |produce_order| %>
      <tr>
        <td><%= produce_order.vendor %></td>
        <td><%= produce_order.orderdate %></td>
        <td><%= link_to 'Show', produce_order %></td>
        <td><%= link_to 'Edit', edit_produce_order_path(produce_order) %></td>
        <td><%= link_to 'Destroy', produce_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Produce order', new_produce_order_path %>
